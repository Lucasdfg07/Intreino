<script>
	$( document ).on('turbolinks:load', function() {
		// Navbar
		$('.click_navbar .close').hide();
		$('.nav_trainer').hide();
		$('.click_navbar').click(function() {
		  $('.nav_trainer').toggle(300);
		  $('.click_navbar .close').toggle(200);
		  $('.click_navbar .open').toggle(200);
		});
	});
</script>