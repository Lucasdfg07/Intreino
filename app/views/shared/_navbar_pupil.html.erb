		<%= image_tag 'menu_nav', class: 'open' %>

		<%= image_tag 'close', class: 'close' %>
	</section>
	      
	<section class="row center-align">
		<section class="col l2 m4 s6 nav_trainer">
			<%= image_tag 'logo2' %>
			<ul>
				<li><%= link_to 'PÃ¡gina Inicial', root_path %></li>
